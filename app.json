{
  "expo": {
    "name": "FirstReactNative",
    "slug": "FirstReactNative",
    "scheme": "first-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo.png",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.jsm.aora"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "9dbe25e5-1b50-4f2d-9902-670bdbdee9d1"
      }
    }
  }
}
